	int isPalindrome(string S)
	{
	    // Your code goes here
	    int flag=-1;
	    int i=0;
	    int n=S.size()-1;
	    while(i<=(n-i))
	    {
	        if(S[i]==S[n-i])
	        {
	            flag=1;
	            i++;
	        }
	        else
	        {
	            flag=0;
	            break;
	        }
	    }
	    if(flag==1)
	    {
	        return 1;
	    }
	    else
	    {
	        return 0;
	    }
	}
